@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="~/assets/img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Jerry Test Web</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Animation library for notifications   -->
    <link href="~/assets/css/animate.min.css" rel="stylesheet" />
    <!--  Light Bootstrap Table core CSS    -->
    <link href="~/assets/css/light-bootstrap-dashboard.css?v=1.4.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="~/assets/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="~/assets/css/pe-icon-7-stroke.css" rel="stylesheet" />
</head>
<body>

    <!-- 左邊清單 -->
    <div class="sidebar" data-color="purple" data-image='@(Url.Content("~/assets/img/sidebar-5.jpg"))'>

        <!--   you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple" -->


        <div class="sidebar-wrapper">
            <div class="logo">
                <a href="#" class="simple-text">
                    Jerry Web
                </a>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="@Url.Action("training", "AIDashBoard")">
                        <i class="pe-7s-bell"></i>
                        <p>training</p>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("testing", "AIDashBoard")">
                        <i class="pe-7s-news-paper"></i>
                        <p>testing</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>

    <!-- 主頁清單 -->

    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">Testing</a>
                </div>
                <div class="collapse navbar-collapse">

                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-left">
                        </ul>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="separator hidden-lg"></li>
                        </ul>
                    </div>
                </div>
            </div>
        </nav>
        <div class="content">
            <div class="container-fluid">
                <!-- table清單區 -->
                <!-- testing 任務 table-->
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">任務清單</h4>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-hover table-striped display" id="testTable">
                                    <thead>
                                        <tr>
                                            <th>狀態</th>
                                            <th>任務名稱</th>
                                            <th>Model名稱</th>
                                            <th>作者</th>
                                            <th>新增時間</th>
                                            <th>是否刪除</th>
                                        </tr>
                                    </thead>
                                    <tbody id="trainData">
                                    </tbody>

                                </table>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- training 任務 table-->
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">model清單</h4>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-hover table-striped display" id="trainTable">
                                    <thead>
                                        <tr>            
                                            <th></th>
                                            <th>Model名稱</th>
                                            <th>演算法名稱</th>
                                            <th>作者</th>
                                            <th>新增日期</th>
                                        </tr>
                                    </thead>
                                    <tbody id="trainData">
                                    </tbody>

                                </table>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <!-- 第二個卡片區 -->
                    <div class="col-md-12">
                        <div class="card ">
                            <div class="header">
                                <h4 class="text">測試資料輸入區</h4>
                                <div class="content">
                                    <div class="row">
                                        <div class="form-group">
                                            <label><font color="black" size="3">使用者名稱</font></label>
                                            <input type="text" class="form-control" placeholder="Username" value="" id="userName">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group">
                                            <label><font color="black" size="3">測試任務名稱</font></label>
                                            <input type="text" class="form-control" placeholder="任務名稱" value="" id="taskName">
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="form-group">
                                            <label><font color="black" size="3">測試路徑</font></label>
                                            <input type="text" class="form-control" placeholder="D:\\xxx\" value="" id="testPath">
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <button type="button" class="btn btn-info btn-fill pull-right" id="addTestTask">新增任務</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
<!--   Core JS Files   -->
<script src="~/assets/js/jquery.3.2.1.min.js" type="text/javascript"></script>
<script src="~/assets/js/bootstrap.min.js" type="text/javascript"></script>
<!--  Charts Plugin -->
<script src="~/assets/js/chartist.min.js"></script>
<!--  Notifications Plugin    -->
<script src="~/assets/js/bootstrap-notify.js"></script>
<!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
<script src="~/assets/js/light-bootstrap-dashboard.js?v=1.4.0"></script>


<!-- Jerry 引用新的table -->
<link href="https://cdn.datatables.net/1.10.24/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/Testing.js"></script>
<link href="~/Content/Jerry.css" rel="stylesheet" />

</html>
